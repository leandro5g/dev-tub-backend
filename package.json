{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev:server": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",
    "start:server": "node app/shared/infra/http/server.js",
    "postinstall": "tsc",
    "build": "tsc",
    "watch-ts": "tsc - w",
    "deploy": "git add . && git commit -m Heroku && git push heroku master"
  },
  "engines": {
    "node": "16.17.0"
  },
  "_moduleAliases": {
    "@modules": "dist/modules",
    "@shared": "dist/shared"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^18.7.6",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "module-alias": "^2.2.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "express": "^4.18.1",
    "googleapis": "^108.0.0",
    "mongoose": "^6.5.2",
    "mongoose-paginate-v2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0"
  }
}
